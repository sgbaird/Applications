Cell[TextData[{
 "Subscripts are made with the key combination \[ControlKey]+",
 StyleBox["hyphen-minus",
  FontSlant->"Italic"],
 " and overbars are made by highlighting the digit character and pressing \
\[ControlKey]+7 followed by and underscore."
}], "Text",
 CellID->1118086339],

Cell[TextData[{
 "Input in ",
 Cell[BoxData[
  TemplateBox[{
    Cell[
     TextData["SymmetryData"]], "paclet:MaXrd/ref/SymmetryData"},
   "RefLink",
   BaseStyle->{"InlineFormula"}]], "InlineFormula"],
 " (and ",
 Cell[BoxData[
  TemplateBox[{
    Cell[
     TextData["SymmetryOperations"]], "paclet:MaXrd/ref/SymmetryOperations"},
   "RefLink",
   BaseStyle->{"InlineFormula"}]], "InlineFormula"],
 ") are automatically run through ",
 Cell[BoxData[
  TemplateBox[{
    Cell[
     TextData["$GroupSymbolRedirect"]], 
    "paclet:MaXrd/ref/$GroupSymbolRedirect"},
   "RefLink",
   BaseStyle->{"InlineFormula"}]], "InlineFormula"],
 ". An advantage is that the entries do not have to be formatted; instead of \
using ",
 Cell[BoxData[
 "\"\<Fd\!\(\*OverscriptBox[\(3\), \(_\)]\)m\>\""], "InlineFormula"],
 ", entering ",
 Cell[BoxData["\"\<Fd-3m\>\""], "InlineFormula"],
 ", ",
 Cell[BoxData["\"\<F d -3 m\>\""], "InlineFormula"],
 ", ",
 Cell[BoxData["\"\<F 41/d -3 2/m\>\""], "InlineFormula"],
 " or even the Hall string of that space group (",
 Cell[BoxData["\"\<F 4d 2 3 -1d\>\""], "InlineFormula"],
 ") will look up the same space group."
}], "Text",
 CellID->1151212725],

Cell[TextData[{
 "One can also use ",
 Cell[BoxData["\"\<Fd-3m:2\>\""], "InlineFormula"],
 " to specify the alternative setting with cell origin 2, or ",
 Cell[BoxData["\"\<R -3 c:r\>\""], "InlineFormula"],
 " to specify the version with rhombohedral axes."
}], "Text",
 CellID->339500947]
}, Open  ]]
